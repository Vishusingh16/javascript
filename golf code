    <title>Golf Code</title>
    <script>
        var name=["Hole-in-one","Eagle","Birdie","Par","Bogey","Double-Bogey","Go-Home"]
function golfscore(par, stroke){
    if(stroke==1){
        return name[0]
    } else if(stroke<= par-2){
        return name[1]

    }else if(stroke==par-1){
    return name[2]
} else if(stroke==par){
    return[3]
}else if(stroke==par+1){
    return[4]

}else if(stroke== par+2){
    return[5]
}
else if(stroke==par+3){
    return[6]
}
}
console.log(golfscore(5,4))

    </script>
</head>
